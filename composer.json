{
    "name": "nevadskiy/laravel-translatable",
    "description": "Add translations to your Eloquent models",
    "license": "MIT",
    "keywords": [
        "translations",
        "translation",
        "localization",
        "model",
        "database",
        "laravel"
    ],
    "homepage": "https://github.com/nevadskiy/laravel-translatable",
    "authors": [
        {
            "name": "Nevadskiy",
            "email": "nevadskiy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^10.5|^11.5",
        "orchestra/testbench": ">=5.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\Translatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nevadskiy\\Translatable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nevadskiy\\Translatable\\TranslatableServiceProvider"
            ]
        }
    }
}
